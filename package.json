  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config=./config/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --mode production --config=./config/webpack.config.js --progress --colors -p",
    "clean": "rmdir /Q /S dist",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix"
